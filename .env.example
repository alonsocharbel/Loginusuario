# Variables de entorno de ejemplo
# Copia este archivo como .env y configura tus valores

# T1 Pay Client ID (obtener de T1)
REACT_APP_T1_PAY_CLIENT_ID=your_client_id_here

# URL de la API (opcional, usa mock por defecto en desarrollo)
REACT_APP_API_URL=http://localhost:3001/api/v1

# URL de T1 Pay (opcional)
REACT_APP_T1_PAY_URL=https://pay.t1pagos.com

# Google Analytics (opcional)
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Forzar uso de API real en desarrollo (opcional)
# REACT_APP_USE_REAL_API=true
